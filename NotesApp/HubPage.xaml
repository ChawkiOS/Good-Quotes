                                </Grid>
                            </GridViewItem>
                            <!--</StackPanel>-->
                            <!--<StackPanel Margin="0" Orientation="Vertical" HorizontalAlignment="Right" VerticalAlignment="Top" Grid.RowSpan="2" Grid.Row="1">-->
                            <GridViewItem Margin="0" ToolTipService.ToolTip="Make a nice picture with a quote" Style="{StaticResource GridViewItemStyle1}">
                                <Grid x:Name="Icon" Height="90" Margin="0" Tapped="Icon_Tapped">
                                    <Grid Width="140" Background="#fcbd01"/>
                                    <Image Source="Assets/Picture.png" Width="90" Margin="0,-10,0,10" HorizontalAlignment="Center" Stretch="Fill"/>
                                    <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Picture Quotes" VerticalAlignment="Bottom" FontFamily="Segoe UI" FontWeight="SemiLight" FontSize="19" Margin="0,0,0,8" Foreground="White"/>
                                </Grid>
                            </GridViewItem>

                            <GridViewItem Margin="0" ToolTipService.ToolTip="Write your own quote" Style="{StaticResource GridViewItemStyle7}">
                                <Grid x:Name="Icon6" Height="90" Tapped="Icon6_Tapped">
                                    <Grid Width="140" Background="#fcbd01"/>
                                    <Image Source="Assets/Add.png" Width="100" Margin="0,-10,0,10" HorizontalAlignment="Center" Stretch="Fill"/>
                                    <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Write Quote" VerticalAlignment="Bottom" FontFamily="Segoe UI" FontWeight="SemiLight" FontSize="19" Margin="0,0,0,8" Foreground="White"/>
                                </Grid>
                            </GridViewItem>
                            <GridViewItem Margin="0" Style="{StaticResource GridViewItemStyle8}" Visibility="Collapsed">
                                <Grid x:Name="Icon7" Height="90">
                                    <Grid Width="140" Background="#fcbd01"/>
                                    <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text=" " VerticalAlignment="Bottom" FontFamily="Segoe UI" FontWeight="Light" FontSize="21" Margin="0,0,0,10" Foreground="#FF272727"/>
                                </Grid>
                            </GridViewItem>
                        </StackPanel>
                    </Grid>
                    
                    
                    <Grid x:Name="ScondGrid" Grid.Column="3" >
                        <Grid.Projection>
                            <PlaneProjection CenterOfRotationX="10" CenterOfRotationY="1" />
                        </Grid.Projection>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="44"/>
                            <RowDefinition Height="143*"/>
                        </Grid.RowDefinitions>
                        <Grid Margin="0,0,0,4" >
                            <Grid Background="Black" Opacity="0.7"/>
                            <TextBlock HorizontalAlignment="Left" Margin="4,0" Foreground="White" TextWrapping="Wrap" Text="Quote of the Day" VerticalAlignment="Top" FontFamily="Segoe UI" FontSize="26" FontWeight="Light"/>
                        </Grid>
                        <Grid x:Name="ToDayGrid" Grid.RowSpan="2" Grid.Row="1" Height="495" VerticalAlignment="Top" Background="#3FFFFFFF">
                            <ListView Name="QuoteOfDayListView" Visibility="Visible" SelectionChanged="QuoteOfDayListView_SelectionChanged" ItemsSource="{Binding QuoteDayCollectionItem}">
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="FrameworkElement">
                                        <Setter Property="Margin" Value="3 3 16 8"/>
                                    </Style>
                                </ListView.ItemContainerStyle>
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <!--List View Item 465-->
                                        <Grid Width="465">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="40"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="20"/>
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="40"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.ColumnSpan="2" Background="Black" Opacity="0.8"/>
                                                <Image HorizontalAlignment="Center" Height="26" VerticalAlignment="Center" Width="24" Source="Assets/HubPage/Star.png"/>
                                                <TextBlock Text="{Binding QuoteTitle}" FontSize="18" FontWeight="SemiBold" VerticalAlignment="Center" Grid.Column="1" Margin="5,0" FontFamily="Segoe UI"/>
                                            </Grid>
                                            <Grid Grid.Row="2" Background="White">
                                                <TextBlock Text="{Binding QuoteAuthor}" FontSize="15" HorizontalAlignment="Left" Foreground="#fc3c01" FontWeight="SemiLight" VerticalAlignment="Top" Grid.Column="1" Margin="8,-2,8,0" FontFamily="Segoe UI"/>
                                            </Grid>
                                            <Grid Grid.Row="1" Background="White">
                                                <TextBlock Text="{Binding QuoteText}" TextWrapping="Wrap" FontSize="18" Foreground="Black" FontWeight="Normal" VerticalAlignment="Center"  Margin="8,5" FontFamily="Segoe UI"/>

                                            </Grid>
                                        </Grid>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                        </Grid>
                    </Grid>
                    
                    <Grid x:Name="ThirdGrid" Grid.Column="5">
                        <Grid.Projection>
                            <PlaneProjection CenterOfRotationX="10" CenterOfRotationY="1" />
                        </Grid.Projection>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="143*"/>
                            </Grid.RowDefinitions>
                            <Grid Margin="0,0,0,4">
                                <Grid Background="Black" Opacity="0.7"/>
                                <TextBlock HorizontalAlignment="Left" Margin="4,0" Foreground="White" TextWrapping="Wrap" Text="Popular Quotes" VerticalAlignment="Top" FontFamily="Segoe UI" FontSize="26" FontWeight="Light"/>
                            </Grid>
                            <Grid Grid.RowSpan="2" Grid.Row="1" Height="495" VerticalAlignment="Top" Background="#3FFFFFFF">
                                <ListView Name="PopularQuotesListView" Visibility="Visible" SelectionChanged="PopularQuotesListView_SelectionChanged" ItemsSource="{Binding PolularQuoteCollectionItem}">
                                    <ListView.ItemContainerStyle>
                                        <Style TargetType="FrameworkElement">
                                            <Setter Property="Margin" Value="3 3 16 8"/>
                                        </Style>
                                    </ListView.ItemContainerStyle>
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <!--List View Item-->
                                            <Grid Width="465">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="20"/>
                                                </Grid.RowDefinitions>
                                                <Grid Grid.Row="1" Background="White">
                                                    <TextBlock Text="{Binding QuoteAuthor}" FontSize="14" HorizontalAlignment="Left" Foreground="Black" FontWeight="SemiLight" VerticalAlignment="Center" Grid.Column="1" Margin="8,-2,8,0" FontFamily="Segoe UI"/>
                                                </Grid>
                                                <Grid Grid.Row="0" >
                                                    <Grid Background="#fcbd01" Opacity="0.8"/>
                                                    <TextBlock Text="{Binding QuoteText}" TextWrapping="Wrap" FontSize="18" Foreground="Black" FontWeight="Normal" VerticalAlignment="Center"  Margin="8,5" FontFamily="Segoe UI"/>
                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                </ListView>
                            </Grid>
                        </Grid>
                    
                </Grid>

                <Grid x:Name="SecondMainGrid" Visibility="Visible" Margin="20,30,40,0" Grid.Row="1">
                    <Grid.RenderTransform>
                        <CompositeTransform TranslateX="{Binding windowWidth}"/>
                    </Grid.RenderTransform>
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="200"/>-->
                        <ColumnDefinition Width="30"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="44"/>
                            <RowDefinition Height="143*"/>
                        </Grid.RowDefinitions>
                        <Grid Margin="0,0,0,4">
                            <Grid Background="Black" Opacity="0.7"/>
                            <TextBlock HorizontalAlignment="Left" Margin="6,0" Foreground="White" TextWrapping="Wrap" Text="Quote Topics" VerticalAlignment="Top" FontFamily="Segoe UI" FontSize="26" FontWeight="Light"/>
                        </Grid>
                        <Grid Grid.RowSpan="2" Grid.Row="1" Height="495" VerticalAlignment="Top" Background="#4FFFFFFF">
                            <GridView Name="gridViewQuotesTopic" IsItemClickEnabled="True" ItemClick="gridViewQuotesTopic_ItemClick" ItemsSource="{Binding TopicsCollectionItem}">
                                <GridView.ItemTemplate>
                                    <DataTemplate>
                                        <Grid Height="43" Width="190" Background="#6FFFFFFF">
                                            <Border BorderBrush="Black" BorderThickness="1"/>
                                            <TextBlock Foreground="Black" VerticalAlignment="Center" HorizontalAlignment="Center" Text="{Binding TopicText}" FontSize="20"/>
                                        </Grid>
                                    </DataTemplate>
                                </GridView.ItemTemplate>
                            </GridView>
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="0" Visibility="Collapsed">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="44"/>
                            <RowDefinition Height="143*"/>
                        </Grid.RowDefinitions>
                        <Grid Margin="0,0,0,4">
                            <Grid Background="Black" Opacity="0.7"/>
                            <TextBlock HorizontalAlignment="Left" Margin="4,0" Foreground="White" TextWrapping="Wrap" Text="Authors" VerticalAlignment="Top" FontFamily="Segoe UI" FontSize="26" FontWeight="Light"/>
                        </Grid>
                        <Grid Grid.RowSpan="2" Grid.Row="1" Height="495" VerticalAlignment="Top" Background="#4FFFFFFF">
                            <Button Name="btnMoreAppBar" Click="btnMoreAppBar_Click" Margin="0,0,0,-5" VerticalAlignment="Bottom" Width="80" Height="65" HorizontalAlignment="Right" Style="{StaticResource MoreAppBarButtonStyle}" Background="White" BorderBrush="White" Foreground="Black" ></Button>

                        </Grid>
                    </Grid>

                </Grid>
                <!--</ScrollViewer>-->
            </Grid>
            <Grid x:Name="grid" HorizontalAlignment="Center" Visibility="Collapsed" Height="100" VerticalAlignment="Center" Width="300">
                <TextBlock x:Name="textBlock" FontWeight="SemiLight" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Please switch to full screen to view the details." FontSize="20" VerticalAlignment="Top"/>
            </Grid>
        </Grid>

        <VisualStateManager.VisualStateGroups>
            <VisualStateGroup>
                <VisualState x:Name="Narrow">
                    <Storyboard>

                        <ObjectAnimationUsingKeyFrames
                    Storyboard.TargetName="full"
                    Storyboard.TargetProperty="Visibility">
                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                        </ObjectAnimationUsingKeyFrames>

                        <ObjectAnimationUsingKeyFrames
                    Storyboard.TargetName="grid"
                    Storyboard.TargetProperty="Visibility">
                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                        </ObjectAnimationUsingKeyFrames>

                    </Storyboard>
                </VisualState>
                <VisualState x:Name="FullScreenPortrait">
                    <Storyboard>

                        <ObjectAnimationUsingKeyFrames
                    Storyboard.TargetName="full"
                    Storyboard.TargetProperty="Visibility">
                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                        </ObjectAnimationUsingKeyFrames>

                        <ObjectAnimationUsingKeyFrames
                    Storyboard.TargetName="grid"
                    Storyboard.TargetProperty="Visibility">
                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                        </ObjectAnimationUsingKeyFrames>

                    </Storyboard>
                </VisualState>
            </VisualStateGroup>
        </VisualStateManager.VisualStateGroups>
    </Grid>
</Page>
